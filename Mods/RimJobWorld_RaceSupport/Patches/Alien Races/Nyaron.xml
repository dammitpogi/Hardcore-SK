<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<!-- Nyaron -->
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Nyaron race</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAttributeSet">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="Alien_Nyaron"]</xpath>
					<attribute>ParentName</attribute>
					<value>BasePawn</value>
				</li>
				<li Class="PatchOperationConditional">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="Alien_Nyaron"]/statBases/Mass</xpath>
					<nomatch Class="PatchOperationAdd">
						<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="Alien_Nyaron"]/statBases</xpath>
						<value>
							<Mass>65</Mass>
						</value>
					</nomatch>
					<match Class="PatchOperationReplace">
						<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="Alien_Nyaron"]/statBases/Mass</xpath>
						<value>
							<Mass>65</Mass>
						</value>
					</match>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>