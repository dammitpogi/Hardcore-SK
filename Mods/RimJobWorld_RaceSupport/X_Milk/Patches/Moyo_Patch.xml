<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Moyo-From the depth</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/HediffDef[defName="MoyoBaseHediff"]/comps/li[1]/hediffToNullify</xpath>
					<value>
						<li>ShallowBlue</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="Alien_Moyo"]/comps</xpath>
					<value>
						<li Class="Milk.CompProperties_MilkableHuman">
							<milkDef>MoyoMilk</milkDef>
							<milkIntervalDays>2</milkIntervalDays>
							<milkAmount>1</milkAmount>
							<milkFemaleOnly>false</milkFemaleOnly>
						</li>
						<li Class="Milk.CompProperties_HyperMilkableHuman">
							<milkDef>MoyoMilk</milkDef>
							<milkIntervalDays>1</milkIntervalDays>
							<milkAmount>2</milkAmount>
							<milkFemaleOnly>false</milkFemaleOnly>
						</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/HediffDef[defName="ShallowBlue"]/descriptionHyperlinks</xpath>
					<value>
						<HediffDef>DeepBlues</HediffDef>
					</value>
				</li>
			</operations>
		</match>
		<nomatch Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationRemove">
					<xpath>/Defs/ThingDef[defName="MoyoMilk"]</xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>/Defs/HediffDef[defName="ShallowBlue"]</xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>/Defs/RecipeDef[defName="Distill_MoyoMilk"]</xpath>
				</li>
			</operations>
		</nomatch>
	</Operation>
</Patch>
