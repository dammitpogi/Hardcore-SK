		<defName>naturalgenius</defName>
		<commonality>.4</commonality>
		<degreeDatas>
			<li>
				<label>Natural Genius</label>
				<description>[PAWN_nameDef] has an extraordinary intellect accompanied with mental instability.</description>
				<statFactors>
					<GlobalLearningFactor>1.5</GlobalLearningFactor>
					<PsychicSensitivity>1.4</PsychicSensitivity>
					<MentalBreakThreshold>1.2</MentalBreakThreshold>
				</statFactors>
			</li>
		</degreeDatas>
	</TraitDef>

	<TraitDef>
		<defName>wise</defName>
		<commonality>.3</commonality>
		<degreeDatas>
			<li>
				<label>Wise</label>
				<description>[PAWN_nameDef] maintains a clear state of mind and remains unfazed by the struggles of life.</description>
				<statFactors>
					<GlobalLearningFactor>1.1</GlobalLearningFactor>
					<ResearchSpeed>1.4</ResearchSpeed>
					<PsychicSensitivity>1.35</PsychicSensitivity>
					<MentalBreakThreshold>0.75</MentalBreakThreshold>
				</statFactors>
				<skillGains>
					<li>
