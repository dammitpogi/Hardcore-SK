<?xml version="1.0" encoding="utf-8" ?>
<Patch>


	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Alpha Animals</li>
		</mods>

		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>



				<li Class="PatchOperationAdd"> 
					<success>Always</success>
					<xpath>/Defs</xpath>
					<value>

						<ItemProcessor.CombinationDef>
							<defName>VFEM_MunitionsAndArmamentFactory_ChemfuelClarifier_RedWoodRecipe</defName>
							<building>VFEM_MunitionsAndArmamentFactory_ChemfuelClarifier</building>
							<items>
								<li>GU_RedWood</li>
							</items>	
							<amount>
								<li>10</li>
							</amount>
							<result>Chemfuel</result>	
							<yield>10</yield>	
							<outputLimitControlled>true</outputLimitControlled>
							<maxTotalOutput>1000</maxTotalOutput>							
							<useQualityIncreasing>false</useQualityIncreasing>
							<singleTimeIfNotQualityIncreasing>0.1</singleTimeIfNotQualityIncreasing>
						</ItemProcessor.CombinationDef>

						<ItemProcessor.CombinationDef>
							<defName>VFEM_ElectronicsFabricationFactory_SteelExtractor_SkyChunksRecipe</defName>
							<building>VFEM_ElectronicsFabricationFactory_SteelExtractor</building>
							<items>
								<li>StoneChunks</li>
							</items>	
							<amount>
								<li>1</li>
							</amount>
							<outputLimitControlled>true</outputLimitControlled>
							<maxTotalOutput>300</maxTotalOutput>
							<result>AA_SkySteel</result>	
							<yield>5</yield>
							<isCategoryRecipe>true</isCategoryRecipe>
							<useQualityIncreasing>false</useQualityIncreasing>
							<singleTimeIfNotQualityIncreasing>0.1</singleTimeIfNotQualityIncreasing>
						</ItemProcessor.CombinationDef>
						<ItemProcessor.CombinationDef>
							<defName>VFEM_VersatileAssembler_UraniumCrystals</defName>
							<building>VFEM_VersatileAssembler</building>
							<items>
								<li>AA_UraniumCrystals</li>
							</items>	
							<secondItems>
								<li>AA_UraniumCrystals</li>
							</secondItems>
							<thirdItems>
								<li>AA_UraniumCrystals</li>
							</thirdItems>
							<amount>
								<li>1</li>
								<li>1</li>
								<li>1</li>
							</amount>
							<outputLimitControlled>true</outputLimitControlled>
							<maxTotalOutput>500</maxTotalOutput>
							<result>Uranium</result>	
							<yield>50</yield>		
							<useQualityIncreasing>false</useQualityIncreasing>
							<singleTimeIfNotQualityIncreasing>0.1</singleTimeIfNotQualityIncreasing>
						</ItemProcessor.CombinationDef>
						<ItemProcessor.CombinationDef>
							<defName>VFEM_VersatileAssembler_Bluegel</defName>
							<building>VFEM_VersatileAssembler</building>
							<items>
								<li>AA_BlueGel</li>
							</items>	
							<secondItems>
								<li>AA_BlueGel</li>
							</secondItems>
							<thirdItems>
								<li>AA_BlueGel</li>
							</thirdItems>
							<amount>
								<li>30</li>
								<li>30</li>
								<li>30</li>
							</amount>
							<outputLimitControlled>true</outputLimitControlled>
							<maxTotalOutput>500</maxTotalOutput>
							<result>AA_Hexagel</result>	
							<yield>20</yield>		
							<useQualityIncreasing>false</useQualityIncreasing>
							<singleTimeIfNotQualityIncreasing>0.1</singleTimeIfNotQualityIncreasing>
						</ItemProcessor.CombinationDef>
						<ItemProcessor.CombinationDef>
							<defName>VFEM_VersatileAssembler_Hexagel</defName>
							<building>VFEM_VersatileAssembler</building>
							<items>
								<li>AA_Hexagel</li>
							</items>	
							<secondItems>
								<li>AA_Hexagel</li>
							</secondItems>
							<thirdItems>
								<li>Uranium</li>
							</thirdItems>
							<amount>
								<li>6</li>
								<li>6</li>
								<li>3</li>
							</amount>
							<resultUsesSpecialIcon>true</resultUsesSpecialIcon>
							<resultSpecialIcon>Things/Item/Resource/AA_HexagelEnergyCore/AA_HexagelEnergyCore_b</resultSpecialIcon>		
							<outputLimitControlled>true</outputLimitControlled>
							<maxTotalOutput>100</maxTotalOutput>
							<result>AA_HexagelCore</result>	
							<yield>5</yield>		
							<useQualityIncreasing>false</useQualityIncreasing>
							<singleTimeIfNotQualityIncreasing>0.1</singleTimeIfNotQualityIncreasing>
						</ItemProcessor.CombinationDef>



					</value>
				</li>







			</operations>
		</match>
	</Operation>


</Patch>

