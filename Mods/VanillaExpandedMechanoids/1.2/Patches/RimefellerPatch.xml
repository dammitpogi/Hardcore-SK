<?xml version="1.0" encoding="utf-8" ?>
<Patch>


	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Rimefeller</li>
		</mods>

		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>



				<li Class="PatchOperationAdd"> 
					<success>Always</success>
					<xpath>/Defs</xpath>
					<value>

						<ItemProcessor.CombinationDef>
							<defName>VFEM_ElectronicsFabricationFactory_ComponentAssembler_SyntheleneRecipe</defName>
							<building>VFEM_ElectronicsFabricationFactory_ComponentAssembler</building>
							<items>
								<li>Synthylene</li>
							</items>	
							<amount>
								<li>16</li>
							</amount>
							<result>ComponentIndustrial</result>	
							<yield>1</yield>
							<outputLimitControlled>true</outputLimitControlled>
							<maxTotalOutput>100</maxTotalOutput>
							<useQualityIncreasing>false</useQualityIncreasing>
							<singleTimeIfNotQualityIncreasing>0.1</singleTimeIfNotQualityIncreasing>
						</ItemProcessor.CombinationDef>

						<ItemProcessor.CombinationDef>
							<defName>VFEM_ElectronicsFabricationFactory_AdvancedAssembler_SyntheleneRecipe</defName>
							<building>VFEM_ElectronicsFabricationFactory_AdvancedAssembler</building>
							<items>
								<li>Synthylene</li>
							</items>	
							<secondItems>
								<li>ComponentIndustrial</li>
							</secondItems>
							<thirdItems>
								<li>Gold</li>
							</thirdItems>
							<fourthItems>
								<li>Plasteel</li>
							</fourthItems>
							<amount>
								<li>40</li>
								<li>1</li>
								<li>3</li>
								<li>10</li>
							</amount>
							<result>ComponentSpacer</result>	
							<yield>1</yield>	
							<outputLimitControlled>true</outputLimitControlled>
							<maxTotalOutput>100</maxTotalOutput>		
							<useQualityIncreasing>false</useQualityIncreasing>
							<singleTimeIfNotQualityIncreasing>0.1</singleTimeIfNotQualityIncreasing>
						</ItemProcessor.CombinationDef>



					</value>
				</li>







			</operations>
		</match>
	</Operation>


</Patch>

